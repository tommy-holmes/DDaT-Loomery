{
  "kind": "collectionType",
  "collectionName": "buildings",
  "info": {
    "singularName": "building",
    "pluralName": "buildings",
    "displayName": "Building",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "shortCode": {
      "type": "string",
      "unique": true
    },
    "floorCount": {
      "type": "integer"
    },
    "buildingId": {
      "type": "biginteger",
      "unique": true
    },
    "floors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::floor.floor",
      "mappedBy": "building"
    },
    "point_of_interests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::point-of-interest.point-of-interest",
      "mappedBy": "building"
    },
    "point_of_interest": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::point-of-interest.point-of-interest"
    }
  }
}
