{
  "kind": "collectionType",
  "collectionName": "point_of_interests",
  "info": {
    "singularName": "point-of-interest",
    "pluralName": "point-of-interests",
    "displayName": "Point of Interest",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "shortName": {
      "type": "string",
      "required": false
    },
    "integerID": {
      "type": "integer",
      "required": false
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "libCalID": {
      "type": "integer"
    },
    "mapLink": {
      "type": "string"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "facilities": {
      "displayName": "facilities",
      "type": "component",
      "repeatable": true,
      "component": "facilities.facilities"
    },
    "studySpaces": {
      "type": "enumeration",
      "enum": [
        "none",
        "bookable",
        "non-bookable",
        "both"
      ],
      "required": true,
      "default": "none"
    },
    "zoomLevel": {
      "type": "integer",
      "max": 22,
      "min": 0
    }
  }
}
