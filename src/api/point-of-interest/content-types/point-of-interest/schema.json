{
  "kind": "collectionType",
  "collectionName": "point_of_interests",
  "info": {
    "singularName": "point-of-interest",
    "pluralName": "point-of-interests",
    "displayName": "Point of Interest",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "shortName": {
      "type": "string",
      "required": false
    },
    "integerID": {
      "type": "integer",
      "required": false
    },
    "latitude": {
      "type": "float",
      "required": false
    },
    "longitude": {
      "type": "float",
      "required": false
    },
    "libCalID": {
      "type": "integer"
    },
    "mapLink": {
      "type": "string"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "studySpaces": {
      "type": "enumeration",
      "enum": [
        "none",
        "bookable",
        "non-bookable",
        "both"
      ],
      "required": true,
      "default": "none"
    },
    "zoomLevel": {
      "type": "integer",
      "max": 22,
      "min": 0
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "cafe / restaurant",
        "building",
        "library",
        "accommodation",
        "bar",
        "car park",
        "transportation",
        "shop / post office",
        "nursery",
        "medical centre",
        "sports facility",
        "student union",
        "cashpoint",
        "prayer room",
        "room",
        "toilet"
      ],
      "required": true
    },
    "priority": {
      "type": "integer",
      "required": false
    },
    "parentPOI": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::point-of-interest.point-of-interest",
      "inversedBy": "childrenPOI"
    },
    "childrenPOI": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::point-of-interest.point-of-interest",
      "mappedBy": "parentPOI"
    },
    "busyness": {
      "type": "component",
      "repeatable": false,
      "component": "busyness.location"
    },
    "description": {
      "type": "text"
    },
    "openingTimesUrl": {
      "type": "string"
    }
  }
}
